@Article{Ridinger2011a,
  Title                    = {Large atom number dual-species magneto-optical trap for fermionic ^6Li and $^{40}\textK$ atoms},
  Author                   = {Ridinger, A. and Chaudhuri, S. and Salez, T. and Eismann, U. and Fernandes, D. and Magalh\~aes, K. and Wilkowski, D. and Salomon, C. and Chevy, F.},
  Journal                  = {The European Physical Journal D - Atomic, Molecular, Optical and Plasma Physics},
  Year                     = {2011},
  Note                     = {10.1140/epjd/e2011-20069-4},
  Pages                    = {223-242},
  Volume                   = {65},

  Abstract                 = {We present the design, implementation and characterization of a dual-species magneto-optical trap (MOT) for fermionic 6 Li and 40 K atoms with large atom numbers. The MOT simultaneously contains 5.2 × 10 9 6 Li-atoms and 8.0 × 10 9 40 K-atoms, which are continuously loaded by a Zeeman slower for 6 Li and a 2D-MOT for 40 K. The atom sources induce capture rates of 1.2 × 10 9 6 Li-atoms/s and 1.4 × 10 9 40 K-atoms/s. Trap losses due to light-induced interspecies collisions of ?65% were observed and could be minimized to ?10% by using low magnetic field gradients and low light powers in the repumping light of both atomic species. The described system represents the starting point for the production of a large-atom number quantum degenerate Fermi-Fermi mixture.},
  Affiliation              = {Laboratoire Kastler Brossel, École Normale Supérieure, CNRS, Université Pierre et Marie-Curie, 24 rue Lhomond, 75231 Paris Cedex 05, France},
  File                     = {Ridinger2011a.pdf:ArticlePDFs\\Ridinger2011a.pdf:PDF},
