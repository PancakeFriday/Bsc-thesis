@Article{Vatasescu2009,
  Title                    = {Formation of cold molecules by shaping with light the short-range interaction between cold atoms: photoassociation with strong laser pulses},
  Author                   = {Mihaela Vatasescu},
  Journal                  = {Journal of Physics B: Atomic, Molecular and Optical Physics},
  Year                     = {2009},
  Number                   = {16},
  Pages                    = {165303},
  Volume                   = {42},

  Abstract                 = {This paper investigates the formation of cold molecules in the photoassociation of two cold atoms by a strong laser pulse applied at short interatomic distances, which lead to molecular dynamics taking place in the light-induced (adiabatic) potentials. A two-electronic-state model in the caesium dimer is used to analyse the effects of this strong coupling regime and to show specific results: (i) acceleration of the ground-state population to the inner zone due to a non-impulsive regime of coupling at short and intermediate interatomic distances, (ii) formation of cold molecules in strongly bound levels of the ground state, where the population at the end of the pulse is much bigger than the population photoassociated in bound levels of the excited state, and (iii) the final momentum distribution of the ground-state wavepacket keeping the signatures of the maxima in the initial wavefunction continuum. It is shown that the topology of the light-induced potentials plays an important role in dynamics.},
  File                     = {Vatasescu2009.pdf:ArticlePDFs\\Vatasescu2009.pdf:PDF},
  Owner                    = {repp},
  Timestamp                = {2011.12.14},
  Url                      = {http://stacks.iop.org/0953-4075/42/i=16/a=165303}
}

