@Article{Meinert2014,
  Title                    = {Observation of many-body dynamics in long-range tunneling after a quantum quench},
  Author                   = {Meinert, Florian and Mark, Manfred J. and Kirilov, Emil and Lauber, Katharina and Weinmann, Philipp and Gröbner, Michael and Daley, Andrew J. and Nägerl, Hanns-Christoph},
  Journal                  = {Science},
  Year                     = {2014},
  Number                   = {6189},
  Pages                    = {1259-1262},
  Volume                   = {344},

  Abstract                 = {Quantum tunneling is at the heart of many low-temperature phenomena. In strongly correlated lattice systems, tunneling is responsible for inducing effective interactions, and long-range tunneling substantially alters many-body properties in and out of equilibrium. We observe resonantly enhanced long-range quantum tunneling in one-dimensional Mott-insulating Hubbard chains that are suddenly quenched into a tilted configuration. Higher-order tunneling processes over up to five lattice sites are observed as resonances in the number of doubly occupied sites when the tilt per site is tuned to integer fractions of the Mott gap. This forms a basis for a controlled study of many-body dynamics driven by higher-order tunneling and demonstrates that when some degrees of freedom are frozen out, phenomena that are driven by small-amplitude tunneling terms can still be observed.},
  Doi                      = {10.1126/science.1248402},
  Eprint                   = {http://www.sciencemag.org/content/344/6189/1259.full.pdf},
  File                     = {Published version:Meinert2014.pdf:PDF},
  Url                      = {http://www.sciencemag.org/content/344/6189/1259.abstract}
}

