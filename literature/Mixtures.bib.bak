@Article{Sofikitis2009,
  Title                    = {Broadband Vibrational Cooling of Cold Cesium Molecules: Theory and Experiments},
  Author                   = {D. Sofikitis and A. Fioretti and S. Weber and M. Viteau and A. Chotia and R. Horchani and M. Allegrini and B. Chatel and D. Comparat and P. Pillet},
