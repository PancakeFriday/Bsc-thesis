@Article{Voigtsberger2014,
  Title                    = {Imaging the structure of the trimer systems $^4${H}e$_3$ and $^3${H}e$^4${H}e$_2$},
  Author                   = {Voigtsberger, J. and Zeller, S. and Becht, J. and Neumann, N. and Sturm, F. and Kim, H.-K. and Waitz, M. and Trinter, F. and Kunitski, M. and Kalinin, A. and Wu, J. and Schöllkopf, W. and Bressanini, D. and Czasch, A. and Williams, J. B. and Ullmann-Pfleger, K. and Schmidt, L. Ph H. and Schöffler, M. S. and Grisenti, R. E. and Jahnke, T. and Dörner, R.},
  Journal                  = {Nat Commun},
  Year                     = {2014},

  Month                    = dec,
  Pages                    = {5765},
  Volume                   = {5},

  Abstract                 = {Helium shows fascinating quantum phenomena unseen in any other element. In its liquid phase, it is the only known superfluid. The smallest aggregates of helium, the dimer (He2) and the trimer (He3) are, in their predicted structure, unique natural quantum objects. While one might intuitively expect the structure of 4He3 to be an equilateral triangle, a manifold of predictions on its shape have yielded an ongoing dispute for more than 20 years. These predictions range from 4He3 being mainly linear to being mainly an equilateral triangle. Here we show experimental images of the wave functions of 4He3 and 3He4He2 obtained by Coulomb explosion imaging of mass-selected clusters. We propose that 4He3 is a structureless random cloud and that 3He4He2 exists as a quantum halo state.},
  Publisher                = {Nature Publishing Group, a division of Macmillan Publishers Limited. All Rights Reserved.},
  Timestamp                = {2015.04.28},
  Url                      = {http://dx.doi.org/10.1038/ncomms6765}
}

@Article{Volosniev2014,
  Title                    = {Borromean ground state of fermions in two dimensions},
  Author                   = {A G Volosniev and D V Fedorov and A S Jensen and N T Zinner},
  Journal                  = {Journal of Physics B: Atomic, Molecular and Optical Physics},
  Year                     = {2014},
  Number                   = {18},
  Pages                    = {185302},
  Volume                   = {47},

  Abstract                 = {The study of quantum mechanical bound states is as old as quantum theory itself. Yet, it took many years to realize that three-body Borromean systems that are bound when any two-body subsystem is unbound are abundant in nature. Here we demonstrate the existence of Borromean systems of spin-polarized (spinless) identical fermions in two spatial dimensions. The ground state with zero orbital (planar) angular momentum exists in a Borromean window between critical two- and three-body strengths. The doubly degenerate first excited states of angular momentum one appears only very close to the two-body threshold. They are the lowest in a possible sequence of so-called super-Efimov states. While the observation of the super-Efimov scaling could be very difficult, the Borromean ground state should be observable in cold atomic gases and could be the basis for producing a quantum gas of three-body states in two dimensions.},
  Url                      = {http://stacks.iop.org/0953-4075/47/i=18/a=185302}
}

