@Article{Kuznetsova2009,
  Title                    = {Efficient formation of ground-state ultracold molecules via STIRAP from the continuum at a {F}eshbach resonance},
  Author                   = {Elena Kuznetsova and Marko Gacesa and Philippe Pellegrini and Susanne F Yelin and Robin C\^ot\'e},
  Journal                  = {New Journal of Physics},
  Year                     = {2009},
  Number                   = {5},
  Pages                    = {055028},
  Volume                   = {11},

  Abstract                 = {We develop a theoretical description of photoassociative stimulated Raman adiabatic passage (STIRAP) near a Feshbach resonance in a thermal atomic gas. We show that it is possible to use low-intensity laser pulses to directly excite pairs of atoms in the continuum near a Feshbach resonance and to transfer most of the atomic cloud to the lowest rovibrational level of the molecular ground state. For a broad resonance, commonly found in several mixtures of alkali atoms, our model predicts a transfer efficiency up to 97% for a given atom pair, and up to 70% when averaged over an atomic ensemble. The laser intensities and pulse durations needed for such an optimal transfer, 10 2 Ã¢â‚¬â€œ10 3 W cm Ã¢Ë†â€™2 and several microseconds, are easily achievable experimentally. A single pair of STIRAP pulses converts an estimated fraction f ~10 Ã¢Ë†â€™6 Ã¢â‚¬â€œ10 Ã¢Ë†â€™4 of atoms in an atomic ensemble, leading to the production of 10Ã¢â‚¬â€œ1000 molecules in a large sample of 10 7 atoms. A total of ~10 4 Ã¢â‚¬â€œ10 6 pulse pairs are thus required to transfer most atoms into molecules. Such an efficiency compares with or surpasses currently available techniques for creating stable diatomic molecules, and the versatility of this approach simplifies its potential use for many molecular species.},
  File                     = {Kuznetsova2009.pdf:ArticlePDFs\\Kuznetsova2009.pdf:PDF},
  Owner                    = {repp},
  Timestamp                = {2011.12.15},
  Url                      = {http://stacks.iop.org/1367-2630/11/i=5/a=055028}
}

