  Title                    = {Two-particle quantum interference in tunnel-coupled optical tweezers},
  Author                   = {Kaufman, A. M. and Lester, B. J. and Reynolds, C. M. and Wall, M. L. and Foss-Feig, M. and Hazzard, K. R. A. and Rey, A. M. and Regal, C. A.},
