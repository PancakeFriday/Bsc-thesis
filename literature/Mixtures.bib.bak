@Article{Sawyer2011,
  Title                    = {Cold heteromolecular dipolar collisions},
  Author                   = {Sawyer, Brian C. and Stuhl, Benjamin K. and Yeo, Mark and Tscherbul, Timur V. and Hummon, Matthew T. and Xia, Yong and Klos, Jacek and Patterson, David and Doyle, John M. and Ye, Jun},
  Journal                  = {Phys. Chem. Chem. Phys.},
  Year                     = {2011},
  Pages                    = {19059-19066},
  Volume                   = {13},

  Abstract                 = {Cold molecules promise to reveal a rich set of novel collision dynamics in the low-energy regime. By combining for the first time the techniques of Stark deceleration{,} magnetic trapping{,} and cryogenic buffer gas cooling{,} we present the first experimental observation of cold collisions between two different species of state-selected neutral polar molecules. This has enabled an absolute measurement of the total trap loss cross sections between OH and ND3 at a mean collision energy of 3.6 cm-1 (5 K). Due to the dipolar interaction{,} the total cross section increases upon application of an external polarizing electric field. Cross sections computed from ab initio potential energy surfaces are in agreement with the measured value at zero external electric field. The theory presented here represents the first such analysis of collisions between a 2[capital Pi] radical and a closed-shell polyatomic molecule.},
  Doi                      = {10.1039/C1CP21203F},
  File                     = {Sawyer2011.pdf:ArticlePDFs\\Sawyer2011.pdf:PDF},
  Issue                    = {42},
  Owner                    = {Vincent},
  Publisher                = {The Royal Society of Chemistry},
  Timestamp                = {2014.03.28},
  Url                      = {http://dx.doi.org/10.1039/C1CP21203F}
}

