@Article{Schollkopf1994,
  Title                    = {Nondestructive Mass Selection of Small van der {W}aals Clusters},
  Author                   = {Sch\"{o}llkopf, Wieland and Toennies, J. Peter},
  Journal                  = {Science},
  Year                     = {1994},
  Number                   = {5189},
  Pages                    = {1345-1348},
  Volume                   = {266},

  Abstract                 = {Clusters of atoms or molecules have been extensively studied by a variety of spectroscopies because of their unusual properties. Experiments with van der Waals clusters of defined sizes are not easily possible because nozzle beam expansions used in their production yield broad size distributions. Moreover, being weakly bound they readily fragment in the commonly used electron impact-ionization mass spectrometer detectors. Here it is shown that light fragile clusters of He, H2, and D2 can be selected and identified nondestructively by diffraction from a transmission grating. The method is universally applicable also to heavier species and well suited for spectroscopic studies.},
  Doi                      = {10.1126/science.266.5189.1345},
  Eprint                   = {http://www.sciencemag.org/content/266/5189/1345.full.pdf},
