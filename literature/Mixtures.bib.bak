@Article{Efimov1973,
  Title                    = {Energy levels of three resonantly interacting particles },
  Author                   = {V. Efimov},
  Journal                  = {Nuclear Physics A },
  Year                     = {1973},
  Number                   = {1},
  Pages                    = {157-188},
  Volume                   = {210},

  Abstract                 = {As shown recently, resonances in the pairwise forces result in the appearance of a specific series of levels in the three-particle spectrum. The number of levels may be very large, they lie near the three-body break-up threshold, have characteristic quantum numbers and are of large spatial size. This work represents a detailed analysis of this effect for various particle mass ratios. The effect is shown to exist at all mass ratios, the resonances acting in the forces of all the pairs or two pairs of particles only. The most favourable for the appearance of the series is the case of one particle being much lighter than the other two, while interacting resonantly with them. The physical pattern of the effect, the level spectrum structure and the spatial structure of the states are studied for all mass ratios. The studies of the effect permits one to draw some qualitative conclusions concerning the character of the overall three-particle level spectrum. },
  Doi                      = {http://dx.doi.org/10.1016/0375-9474(73)90510-1},
  File                     = {Published version:Efimov1973.pdf:PDF},
  ISSN                     = {0375-9474},
