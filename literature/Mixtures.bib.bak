@Article{Lignier2011,
  Title                    = {Deeply bound cold caesium molecules formed after 0-g resonant coupling},
  Author                   = {Lignier, H. and Fioretti, A. and Horchani, R. and Drag, C. and Bouloufa, N. and Allegrini, M. and Dulieu, O. and Pruvost, L. and Pillet, P. and Comparat, D.},
  Journal                  = {Phys. Chem. Chem. Phys.},
  Year                     = {2011},
  Pages                    = {18910-18920},
  Volume                   = {13},

  Abstract                 = {Translationally cold caesium molecules are created by photoassociation below the 6s + 6p1/2 excited state and selectively detected by resonance enhanced two photon ionization (RE2PI). A series of excited vibrational levels belonging to the 0-g symmetry is identified. The regular progression of the vibrational spacings and of the rotational constants of the 0-g (6s + 6p1/2) levels is strongly altered in two energy domains. These deviations are interpreted in terms of resonant coupling with deeply bound energy levels of two upper 0-g states dissociating into the 6s + 6p3/2 and 6s + 5d3/2 asymptotes. A theoretical model is proposed to explain the coupling and a quantum defect analysis of the perturbed level position is performed. Moreover{,} the resonant coupling changes dramatically the spontaneous decay products of the photoexcited molecules{,} strongly enhancing the decay into deeply bound levels of the a3[capital Sigma]+u triplet state and of the X1[capital Sigma]+g ground state. These results may be relevant when conceiving population transferring schemes in cold molecule systems.},
  Doi                      = {10.1039/C1CP21488H},
  File                     = {Lignier2011.pdf:ArticlePDFs\\Lignier2011.pdf:PDF},
  Issue                    = {42},
  Owner                    = {Vincent},
  Publisher                = {The Royal Society of Chemistry},
  Timestamp                = {2014.03.28},
  Url                      = {http://dx.doi.org/10.1039/C1CP21488H}
}

