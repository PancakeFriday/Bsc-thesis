@Article{Partridge2006,
  Title                    = {Pairing and Phase Separation in a Polarized Fermi Gas},
  Author                   = {Partridge, Guthrie B. and Li, Wenhui and Kamar, Ramsey I. and Liao, Yean-an and Hulet, Randall G.},
  Journal                  = {Science},
  Year                     = {2006},
  Number                   = {5760},
  Pages                    = {503--505},
  Volume                   = {311},

  Abstract                 = {We report the observation of pairing in a gas of atomic fermions with unequal numbers of two components. Beyond a critical polarization, the gas separates into a phase that is consistent with a superfluid paired core surrounded by a shell of normal unpaired fermions. The critical polarization diminishes with decreasing attractive interaction. For near-zero polarization, we measured the parameter β = {\textendash}0.54 {\textpm} 0.05, describing the universal energy of a strongly interacting paired Fermi gas, and found good agreement with recent theory. These results are relevant to predictions of exotic new phases of quark matter and of strongly magnetized superconductors.},
  Doi                      = {10.1126/science.1122876},
  Eprint                   = {http://science.sciencemag.org/content/311/5760/503.full.pdf},
  File                     = {Published version:Partridge2006.pdf:PDF},
  ISSN                     = {0036-8075},
  Publisher                = {American Association for the Advancement of Science},
