@Article{Wang2014a,
  Title                    = {Universal van der {W}aals physics for three cold atoms near {F}eshbach resonances},
  Author                   = {Wang, Yujun and Julienne, Paul S.},
  Journal                  = {Nat Phys},
  Year                     = {2014},

  Month                    = aug,
  Pages                    = {768–773},
  Volume                   = {10},

  Abstract                 = {Experimental studies with cold atoms have advanced our understanding of three-body physics, historically a fundamental yet challenging problem. This is because atomic interactions can be precisely varied in strength using magnetically tunable scattering resonances known as Feshbach resonances. Collisions near the unitarity limit, where scattering is maximum, are known to have universal aspects that are independent of short-range chemical details. Away from this limit, many quantum states are expected to be active during a three-body collision, making the collisional observables practically unpredictable. Here we predict three-body ultracold scattering rates by properly building in the pairwise van der Waals interactions plus the multi-spin properties of a tunable Feshbach resonance state characterized by known dimensionless two-body parameters. Numerically solving the Schrodinger equation then quantitatively determines three-atom collisional properties at all interaction strengths without needing adjustable parameters to fit data. Consequently, we can define a new class of van der Waals universality for cold atom three-body phenomena.},
  Doi                      = {doi:10.1038/nphys3071},
  File                     = {Published version:Wang2014a.pdf:PDF;:ArticlePDFs\\Wang2014a_arxiv.pdf:PDF},
  ISSN                     = {1745-2481},
  Owner                    = {ulmanis},
  Publisher                = {Nature Publishing Group},
  Timestamp                = {2014.09.10},
  Url                      = {http://dx.doi.org/10.1038/nphys3071}
}

